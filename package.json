{
  "name": "sonar-cli",
  "version": "0.2.59",
  "description": "SonarQube CLI for AI coding agents",
  "type": "module",
  "bin": {
    "sonar": "./dist/sonar-cli"
  },
  "scripts": {
    "build": "tsc",
    "build:binary": "bun build src/index.ts --compile --outfile dist/sonar-cli",
    "dev": "bun run src/index.ts",
    "start": "bun run src/index.ts",
    "test": "bun test tests/unit/**/*.test.ts",
    "test:integration": "bun test tests/integration/**/*.test.ts",
    "test:all": "bun test tests/**/*.test.ts",
    "typecheck": "tsc --noEmit",
    "validate": "npm run spec:validate && npm run commands:validate",
    "spec:validate": "node scripts/validate-spec.js",
    "commands:validate": "node scripts/validate-commands.js",
    "version:update": "bash scripts/update-version.sh",
    "gen": "bash scripts/generate.sh",
    "gen:command": "plop command",
    "gen:docs": "plop docs",
    "gen:all": "plop all-commands",
    "gen:sync": "plop sync-index"
  },
  "dependencies": {
    "@toon-format/toon": "^2.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ink": "^4.4.1",
    "ink-progress-bar": "^3.0.0",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "jayson": "^4.1.0",
    "keytar": "^7.9.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "ajv": "^8.17.1",
    "js-yaml": "^4.1.1",
    "plop": "^4.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
